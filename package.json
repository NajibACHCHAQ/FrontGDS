    "gs-api:download": "copy /Y /B \"C:\\Users\\najib\\Desktop\\Projet APPGDS Front + Back\\API-GDS2\\target\\gestiondestock-0.0.1-SNAPSHOT.jar\" \"./tools/swagger/dist\" && copy /Y /B \"C:\\Users\\najib\\Desktop\\Projet APPGDS Front + Back\\API-GDS2\\target\\swagger.json\" \"./tools/swagger/src\"",
    "gs-api:extract": "node ./tools/swagger/src/swagger.json 0.0.1-SNAPSHOT",
    "gs-api:swagger-gen": "node_modules\\.bin\\ng-swagger-gen.cmd -i ./tools/swagger/src/swagger.json -o src/gs-api/src"

